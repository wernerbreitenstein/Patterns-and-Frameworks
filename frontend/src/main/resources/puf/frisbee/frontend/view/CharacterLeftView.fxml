<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1" fx:controller="puf.frisbee.frontend.view.CharacterLeftView">
 <children>
     <Label fx:id="labelCharacterLeft" />
      <ImageView fitHeight="225.0" fitWidth="300.0" onMouseClicked="#handleCharacterLeftClicked" pickOnBounds="true" preserveRatio="true">
